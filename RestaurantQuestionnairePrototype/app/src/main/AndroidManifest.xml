<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.comrade_ding.restaurantquestionnaireprototype">

    <application
        android:allowBackup="true"
        android:background="@color/colorLight"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".UserPreferencesActivity" />
        <activity android:name=".StartPromptActivity" />
        <activity android:name=".BeginQuestionnaireActivity" />
        <activity android:name=".DisplayQuestionActivity" />
        <activity android:name=".DisplaySuggestionActivity" />
        <activity android:name=".OutOfOptionsActivity"></activity>
    </application>

</manifest>